{
  "name": "nikic/fast-route",
  "description": "Fast request router for PHP",
  "keywords": [
    "routing",
    "router"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Nikita Popov",
      "email": "nikic@php.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "FastRoute\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FastRoute\\Benchmark\\": "benchmark/",
      "FastRoute\\Test\\": "test/"
    }
  },
  "require": {
    "php": ">=8.0",
    "fig/http-message-util": "^1.1",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "phpbench/phpbench": "*",
    "phpstan/phpstan": "^1.5.4"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "scripts": {
  },
  "config": {
    "bin-dir": "bin",
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
